x = np.linspace(1.0, 100.0, 100)
x
y1 = x**2
y2 = 2**x
plt.plot(x, y1, 'g.', label="Squared")
plt.plot(x, y2, 'r.', label="Exponential")
plt.plot(x, np.log(y1), 'g.', label="Squared")
plt.plot(x, np.log(y2), 'r.', label="Squared")
